/* -- undo browser HTML defaults -- */
:link,:visited {text-decoration: none;}
ul,ol {list-style: none;}
ul,ol,dl,li,dt,dd,h1,h2,h3,h4,h5,h6,pre,form,body,html,p,blockquote,fieldset,input {margin: 0; padding: 0;}
a img,:link img,:visited img {border: none;}
address {font-style: normal;}


body {
	font-family: Arial, Lucida, Helvetica, sans-serif;
	padding: 0;
	margin: 0;
	color: #444;
	text-align:center;
}


/*--------------- MAIN CONTENT Structural Styles -----------------
________________________________________________________________*/

#content {
	margin: 0;
	padding: 0 24px 0 23px;
	background: transparent url(images/background/ornament-right.gif) no-repeat 99.9% 230px;
	overflow:hidden;
}

#container {
  width: 940px;
  padding: 20px 0 0 0;
  margin: 0 auto;
  text-align: left;
}

/* ------ hold site content - header-footer-leftsidebar-main content ----------*/
#holds-content {
width: 759px;
}

/* --- Used to place left side ornament graphic --- */
#inner-wrapper {
  background: transparent url(images/background/ornament-left.gif) no-repeat bottom left;
}


/* -- BASIC PAGE & TYPE STYLES --
______________________________________*/

/* ------ Link Styles ----------*/

a { text-decoration: none; }
a:hover, a:focus { text-decoration: none; }

/* ------ Heading Styles ----------*/

h1 { font-size: 1.3em; }
h2 { font-size: 1.1em; }
h3 { font-size: 1em; font-weight: bold;}
h4, h5, h6 { font-size: 0.9em; font-weight: bold;}
h1, h2, h3, h4, h5, h6 {}
h3.form-title {
  text-align: left;
}

/* ------ Type Styles ----------*/

p {
  padding: 0 0 0.5em 0;
  margin: 0;
  text-indent: 0;
  font-size: 1em;
  line-height: 1.3em;
}
pre {
  padding: 0.75em 1.5em;
  font-size: 12px;
}

blockquote {
  margin: 0.5em 1em;
  padding: 0.3em;
  font-style: italic;
}
code {
  font-size: 1em;
  font-family: Profont, 'lucida console', 'Courier New', Monaco, monospace;
}
h1.title {
	margin:10px 0 15px;
	font-size:1.4em;
	letter-spacing:0.05em;
	text-transform:uppercase;
	font-weight:normal;
}

/* ---- Default Page Type Styles ---- */
.node .content h1 { 
  font-size: 1.4em !important;
  text-transform: uppercase;
  color: #D294CE;
  font-weight: normal;
}
.node .content h2 { 
  font-size: 1.3em !important;
  text-transform: uppercase;
  color: #D294CE;
  font-weight: normal;
}
.node .content h3 { 
  font-size: 1.2em !important;
  text-transform: uppercase;
  font-weight: normal;
  font-family: times, serif;
}
.node .content h4 { 
  font-size: 1em !important;
  text-transform: uppercase;
  font-weight: normal;
  color: #D294CE;
}
.node .content h5 { 
  font-size: .98em !important;
  text-transform: uppercase;
  font-weight: normal;
  color: #D294CE;
}
.node .content p { font-family: Georgia, times, serif; font-size: 1.3em !important; }

.node .content p.page-intro {
